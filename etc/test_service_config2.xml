<!--服务器配置-->
<root>
	<!-- 服务基本信息 sync_log-(日志是否异步输出,0表示同步1表示异步) work_path(工作目录) service_name(服务名字，集群内唯一)-->
	<service_info sync_log = "0" work_path = "./conf/" service_type = "test2" service_name = "test2-1" max_socket_count = "100000" host = "127.0.0.1" port = "6100" send_buf_size = "1024" recv_buf_size = "1024">
		<misc invok_timeout = "100000" heartbeat_limit = "100000" heartbeat_time = "10000"/>
	</service_info>
	<time year = "2013" month = "4" day = "27" hour = "23" minute = "57" second = "0" enable = "0"/>
	<!--连接master地址-->
	<connect_master_addr host = "127.0.0.1" port = "10000"/>
</root>